        {
            "name": "exmples/full",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/full",
            "cwd": "${workspaceFolder}/examples/full",
            "env": {},
            "args": ["-default-config", "toml", "-config-file", "config/defaults"]
        }
