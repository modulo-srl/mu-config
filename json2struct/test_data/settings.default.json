{
    // Default settings example.
    // Comments are allowed :)

    "Basic-Types" : {
        "ParamString": "ParamValue \n \"test\"",  // string value

        "ParamBool": true,  // boolean value

        "ParamInt": 12,  // numeric value
        "ParamFloat": 1.234  // numeric value
    },

    "Array-Types": {
        "ArrayInt": [ 1, 2, 3 ],
        "ArrayBool": [ true, false, true ],
        "ArrayFloat": [ 1.0, 2.0, 3.0 ],
        "ArrayString": [ "a", "b", "c" ],
        "ArrayMixed": [ "a", 1, 2.0 ]  // will be converted to strings array
    }
}
